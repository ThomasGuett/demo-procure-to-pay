      <bpmn:serviceTask id="Activity_0q7y8q6" name="inventory system 1" zeebe:modelerTemplate="io.camunda.connectors.Jdbc.v1" zeebe:modelerTemplateVersion="3" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
