          <zeebe:input source="=0.1" target="provider.openaiCompatible.model.parameters.topP" />
          <zeebe:input source="=&#34;You are a inventoryAssistant searching the inventory per user request.&#10;&#10;Mandatory tool driven workflow. Inspect the tools at your disposal to check if the requested tools can be located within your inventory.&#10;&#10;Search through all available inventory lists and check if a similar item can be found. If not the exact but a similar item can be found, suggest the similar item.&#10;&#10;Wrap minimal, inspectable reasoning in *exactly* this XML template:&#10;&#10;&#60;thinking&#62;&#10;&#60;context&#62;…briefly state the customer’s need and current state…&#60;/context&#62;&#10;&#60;reflection&#62;…list candidate tools, justify which you will call next and why…&#60;/reflection&#62;&#10;&#60;/thinking&#62;&#34;" target="data.systemPrompt.prompt" />
          <zeebe:input source="=inventory_request" target="data.userPrompt.prompt" />
          <zeebe:input target="agentContext" />
          <zeebe:input source="in-process" target="data.memory.storage.type" />
          <zeebe:input source="=20" target="data.memory.contextWindowSize" />
          <zeebe:input source="=10" target="data.limits.maxModelCalls" />
          <zeebe:input source="WAIT_FOR_TOOL_CALL_RESULTS" target="data.events.behavior" />
          <zeebe:input source="text" target="data.response.format.type" />
          <zeebe:input source="=true" target="data.response.format.parseJson" />
          <zeebe:input source="=false" target="data.response.includeAssistantMessage" />
          <zeebe:input source="=true" target="data.response.includeAgentContext" />
          <zeebe:output source="=agent" target="agent" />
