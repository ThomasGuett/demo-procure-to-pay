    <bpmn:adHocSubProcess id="Activity_07n7dgb" name="check if in inventory" zeebe:modelerTemplate="io.camunda.connectors.agenticai.aiagent.jobworker.v1" zeebe:modelerTemplateVersion="8" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
